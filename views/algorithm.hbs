<h1>Algos</h1>

{{#if admin}}
<p>Upload file</p>
<form action="/algorithms" method="POST" enctype="multipart/form-data">
    <input type="file" name="choose_btn" required>
    <button type="submit" name="upload_btn">Upload</button>
</form>
{{/if}}

<div class="code-list">
    <ul>
        {{#each files}}
        <li><a href="/algorithms/{{id}}">{{name}}</a></li>
        {{/each}}
    </ul>
</div>